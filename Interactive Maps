   "source": [
    "# Create a base map\n",
    "m_4 = folium.Map(location=[35,136], tiles='cartodbpositron', zoom_start=5)\n",
    "\n",
    "# Your code here: create a map\n",
    "# Filter earthquakes in Japan (near prefectures geometry)\n",
    "earthquakes_gdf = gpd.GeoDataFrame(\n",
    "    earthquakes,\n",
    "    geometry=gpd.points_from_xy(earthquakes['Longitude'], earthquakes['Latitude']),\n",
    "    crs=\"EPSG:4326\"\n",
    ")\n",
    "earthquakes_in_japan = earthquakes_gdf[\n",
    "    earthquakes_gdf.geometry.within(prefectures.unary_union)\n",
    "]\n",
    "# Add choropleth for population density\n",
    "Choropleth(\n",
    "    geo_data=prefectures.__geo_interface__,\n",
    "    data=stats['density'],\n",
    "    key_on=\"feature.id\",\n",
    "    fill_color='YlGnBu',\n",
    "    legend_name=\"Population Density (per sq. km)\"\n",
    ").add_to(m_4)\n",
    "# Extract latitude, longitude, and magnitude for the heatmap\n",
    "heatmap_data = earthquakes_in_japan.geometry.apply(lambda point: [point.y, point.x]).tolist()\n",
    "heatmap_data = [\n",
    "    [coords[0], coords[1], mag]\n",
    "    for coords, mag in zip(heatmap_data, earthquakes_in_japan['Magnitude'])\n",
    "]\n",
    "\n",
    "# Add a heatmap for earthquake magnitudes\n",
    "HeatMap(\n",
    "    data=heatmap_data,\n",
    "    radius=10,\n",
    "    gradient={0.2: 'blue', 0.5: 'lime', 1: 'red'},\n",
    "    max_zoom=12\n",
    ").add_to(m_4)\n",
    "# Uncomment to see a hint\n",
    "#q_4.a.hint()\n",
    "\n",
    "# View the map\n",
    "embed_map(m_4, 'q_4.html')"
   ]
  },
